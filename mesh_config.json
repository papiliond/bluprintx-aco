                    "methods": ["GET", "POST"],
                    "origin": "*"
                },
                "headers": {
                    "mode": "no-cors",
                    "x-include-metadata": "true"
                },
                "includeHTTPDetails": false
            },
            "sources": [
                {
                    "name": "CommerceSaaSGraphQl",
                    "handler": {
                        "graphql": {
                            "endpoint": "https://na1-sandbox.api.commerce.adobe.com/VofEvVSTqApWgHt4qH5NNV/graphql",
                            "useGETForQueries": true,
                            "operationHeaders": {
                                "Content-Type": "application/json",
